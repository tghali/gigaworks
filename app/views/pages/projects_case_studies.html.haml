  jQuery.noConflict();
  jQuery(document).ready(function(){ 
  	jQuery(".div_li_tab").click(function () {
    jQuery(".div_li_tab a").each( function(id){
    Cufon.replace(this, {color: '#465561'});
    });
  		tab_id = jQuery(this).find("a").attr('id');
  		jQuery(".tabdiv_new").hide();
    exist_one = jQuery(".selettab").attr("id");
    jQuery(".div_li_tab_last").css('border-right', '1px solid #c9c9c9');
    jQuery(".div_li_tab_first").css('border-left', '1px solid #c9c9c9');
    jQuery("#"+exist_one).removeClass("selettab");
    jQuery(this).find("a").addClass("selettab");
    document.getElementById("tab"+tab_id).style.display='block';
    if(jQuery(this).attr("class") == "div_li_tab div_li_tab_first"){
    jQuery(this).find("a").css('border-left', '0px solid #000');
    jQuery(this).css('border-left', '0px solid #000');
    }
    if(jQuery(this).attr("class") == "div_li_tab div_li_tab_last"){
    jQuery(this).find("a").css('border-right', '0px solid #000');
    jQuery(this).css('border-right', '0px solid #000');
    }
    });

  	jQuery("#2").click(function () {
  		tab_id = jQuery(this).find("a").attr('id');
  		jQuery(".tabdiv_new").hide();
  		document.getElementById("tab"+tab_id).style.display='block';
  		//jQuery("#tab"+tab_id).show("slow");
  	});
   
  });



