

= javascript_include_tag "frontend/jquery.pikachoose.js"



:javascript
  jQuery(document).ready(function() {
  	
  	jQuery("ul.gallery li, .jcarousel-skin-tango li").hover(function() { //On hover...
  		
  		var thumbOver = jQuery(this).find("img").attr("src"); //Get image url and assign it to 'thumbOver'
  		
  		//Set a background image(thumbOver) on the <a> tag 
  		jQuery(this).find("a.thumb").css({'background' : 'url(' + thumbOver + ') no-repeat center bottom'});
  		//Fade the image to 0 
  		jQuery(this).find("span").stop().fadeTo('normal', 0 , function() {
  			jQuery(this).hide() //Hide the image after fade
  		}); 
  	} , function() { //on hover out...
  		//Fade the image to 1 
  		jQuery(this).find("span").stop().fadeTo('normal', 1).show();
  	});
  
  });

:javascript
  jQuery(document).ready(function (){
  jQuery("#pikame").PikaChoose({carousel:true});
  });

:javascript
  jQuery(document).ready(function() {
  jQuery.noConflict();
  jQuery("ul.fblinks li,p.foot_logo").hover(function() { //On hover...
  var thumbOver = jQuery(this).find("img").attr("src"); //Get image url and assign it to 'thumbOver'
  jQuery(this).find("a.thumb1").css({'background' : 'url(' + thumbOver + ') no-repeat center -23px'});
  jQuery(this).find("a.thumb2").css({'background' : 'url(' + thumbOver + ') no-repeat center -98px'});
  jQuery(this).find("span").stop().fadeTo('normal', 0 , function() {
  jQuery(this).hide(); //Hide the image after fade
  }); 
  } , function() { //on hover out...
  		//Fade the image to 1 
        jQuery(this).find("span").stop().fadeTo('normal', 1).show();
        jQuery(this).find("a.thumb2").stop().css({'background' : 'url(' + thumbOver + ') no-repeat center -96px'});
  });
  });



